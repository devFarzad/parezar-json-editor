{
  "CORSRules": [
    {
      "AllowedHeaders": [
        "*"
      ],
      "AllowedMethods": [
        "GET",
        "PUT",
        "POST",
        "DELETE",
        "HEAD"
      ],
      "AllowedOrigins": [
        "http://localhost:3000",
        "https://localhost:3000",
        "https://*.vercel.app",
        "https://your-production-domain.com"
      ],
      "ExposeHeaders": [
        "ETag",
        "x-amz-request-id"
      ],
      "MaxAgeSeconds": 3000
    }
  ]
} 